<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"  
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	
	<typeAliases>
		<typeAlias alias="user" type="com.kh.eeum.domain.User" />
		<typeAlias alias="expert" type="com.kh.eeum.domain.Expert" />
		<typeAlias alias="oneday" type="com.kh.eeum.domain.Oneday"/>
		<typeAlias alias="admin" type="com.kh.eeum.domain.Admin"/>
		<typeAlias alias="apply" type="com.kh.eeum.domain.Apply"/>
		<typeAlias alias="like" type="com.kh.eeum.domain.Like"/>
	</typeAliases>

	<mappers>
		<mapper resource="sql/user.xml" />
		<mapper resource="sql/expert.xml" />
		<mapper resource="sql/oneday.xml"/>	
		<mapper resource="sql/admin.xml"/>
		<mapper resource="sql/apply.xml"/>
		<mapper resource="sql/like.xml"/>
	</mappers>
</configuration>